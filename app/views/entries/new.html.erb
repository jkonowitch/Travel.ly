<%= form_for [@stop, @entry], :url => stop_entries_path, :html => { :multipart => true } do |form| %>
  <%= form.label :photo %>
  <%= form.file_field :photo %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <%= form.label :text %>
  <%= form.text_area :text %>
  <%= form.submit %>
<% end %>